{
    "name": "liver-seg",
    "version": "1.0.0",
    "description": "MONAI AI Liver Seg",
    "informatics_gateway": {
        "ae_title": "MONAI",
        "data_origins": [
            "test"
        ],
        "export_destinations": [
            "ORTHANC"
        ]
    },
    "tasks": [
        {
            "id": "liver-seg",
            "description": "liver segmentation",
            "type": "argo",
            "args": {
                "namespace": "argo",
                "workflow_template_name": "liver-seg-argo-template",
                "server_url": "https://argo-server.argo:2746",
                "allow_insecure": "true"
            },
            "ref": "",
            "task_destinations": [
                {
                    "name": "export-report",
                    "conditions": ""
                }
            ],
            "export_destinations": [],
            "artifacts": {
                "input": [
                    {
                        "name": "input-dicom",
                        "value": "{{ context.input.dicom }}",
                        "mandatory": true
                    }
                ],
                "output": [
                    {
                        "name": "output",
                        "value": "{{ context.executions.liver-seg.output_dir }}",
                        "mandatory": true
                    }
                ]
            },
            "input_parameters": null
        },
        {
            "id": "export-report",
            "description": "export report",
            "type": "export",
            "args": {
                "test": "test1"
            },
            "ref": "",
            "task_destinations": [],
            "export_destinations": [
                {
                    "name": "ORTHANC"
                }
            ],
            "artifacts": {
                "input": [
                    {
                        "name": "MONAI_OUTPUTPATH",
                        "value": "{{ context.executions.liver-seg.artifacts.MONAI_OUTPUTPATH }}",
                        "mandatory": true
                    }
                ],
                "output": []
            },
            "input_parameters": null
        }
    ]
}